			}
			
			}
		public static int performIntegerOperation(int value) {

		    // Perform operations on integer value
		    // Example:
		    return value + 5;
		}
		public static double performDoubleOperation(double value) {

		    // Perform operations on double value
		    // Example:
		    return value + 2.5;
		}
	
