package automation;
import java.util.HashSet;
import java.util.List;
import java.util.Random;
import java.util.Set;
import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.interactions.Actions;
public class IndentCheck {
public void indentChecking(WebDriver driver) throws InterruptedException {

		
		for (int i = 0; i < 1; i++) {
		//	calling indentInserting method from the same class 
			indentInserting(driver);
			Thread.sleep(2000);
			System.out.println("on loop no.:"+i);
		}
		System.out.println("out of loop");
	}

		public void indentInserting(WebDriver driver) throws InterruptedException {
		//clicking on calendar date 
		driver.findElement(By.id("expected-delivery-date")).click();
		Thread.sleep(2000);
		//selecting some date in calendar field 
		WebElement calendardate = driver.findElement(By.xpath("//*[@id='mat-datepicker-1']/div/mat-month-view/table/tbody/tr[5]/td[6]/button"));
		calendardate.click();
		//clicking on indent purpose 
		driver.findElement(By.id("indentPurposeId")).click();
		Thread.sleep(2000);
		//selecting a random option from that options or fileds or elements 
		List<WebElement> indenttype = driver.findElements(By.xpath("//*[@id='indentPurposeId-panel']//mat-option"));
		Random random = new Random(); 																										//creating random class object to select a random option 
		int index = random.nextInt(indenttype.size());																						// how much size is there i am storing in some datatype variable 
		WebElement randomElement = indenttype.get(index);																					//getting some random option 
		String indenttypeid = randomElement.getAttribute("id");																				//storing that option in a string variables
		randomElement.click();
		Thread.sleep(2000);
		//selecting  radiobutton options from that all radio buttons options 
		List<WebElement> indentcatgory = driver.findElements(By.xpath("//*[@id='indent-category']//mat-radio-button"));
		int index1 = random.nextInt(indentcatgory.size());
		WebElement randomElement1 = indentcatgory.get(index1);
		randomElement1.click();
		String indentcatgoryid = randomElement1.getAttribute("id");
		Thread.sleep(2000);
		//if the radiobutton is selected randomly  1st option means 
		if (indentcatgoryid.equals("indent-category-option-1")) {
			driver.findElement(By.id("company-type")).click();
			Thread.sleep(2000);
			List<WebElement> companytype = driver.findElements(By.xpath("//*[@id='company-type-panel']//mat-option"));
			int index3 = random.nextInt(companytype.size());
			WebElement randomElement3 = companytype.get(index3);
			randomElement3.click();
			String companytypeid = randomElement3.getAttribute("id");
			Thread.sleep(2000);
			driver.findElement(By.id("customer-id")).click();
			List<WebElement> selectcustomer = driver.findElements(By.xpath("//*[@id='customer-id-panel']//mat-option"));
			int index5 = random.nextInt(selectcustomer.size());
			WebElement randomElement5 = selectcustomer.get(index5);
			randomElement5.click();
			String selectcustomerid = randomElement5.getAttribute("id");
			Thread.sleep(2000);
			driver.findElement(By.id("project-id")).click();
			List<WebElement> projecttype = driver.findElements(By.xpath("//*[@id='project-id-panel']//mat-option"));
			Thread.sleep(2000);
			int index7 = random.nextInt(projecttype.size());
			WebElement randomElement7 = projecttype.get(index7);
			String projecttypeid = randomElement7.getAttribute("id");
			randomElement7.click();
			Thread.sleep(2000);
			driver.findElement(By.id("study-list")).click();
			List<WebElement> studylist = driver.findElements(By.xpath("//*[@id='study-list-panel']//mat-option"));
			//int index9 = random.nextInt(studylist.size());
			WebElement randomElement9 = studylist.get(random.nextInt(studylist.size()));
			randomElement9.click();
			String studylistid = randomElement9.getAttribute("id");
			Thread.sleep(2000);
		}
		//if the radiobutton is selected randomly  other  options means 
		//we have to create a set string for store the items and  not  to allow duplicate values 
		Set<String> s = new HashSet<String>();
		for (int i = 0; i < 5; i++) {
			// driver.findElement(By.id("item-name")).click();
			//clicking on item feild 
			WebElement inputField = driver.findElement(By.id("item-name"));
			inputField.sendKeys("a");
			Thread.sleep(3000);
			//selecting a random option from that options or fileds or elements 
			List<WebElement> itemname = driver.findElements(By.xpath(" 	//*[@id='mat-autocomplete-0']//mat-option"));
			int indexitem = random.nextInt(itemname.size());
			System.out.println(indexitem);
			WebElement randomElementitem = itemname.get(indexitem);
			String itemnames = randomElementitem.getText();
			System.out.println(itemnames);
			randomElementitem.click();
			Thread.sleep(3000);
			if (s.add(itemnames) == false) {
				System.out.println("duplicate value :" + indexitem);
				driver.findElement(By.id("reset-item-btn")).click();
				i--;
				continue;
			}
			s.add(itemnames);
			if (i == 0) {
				//giving random generator value up to 25 of integer 
            	WebElement itemqty1 = driver.findElement(By.id("item-request-quantity"));
        		int randomNum1 = random.nextInt(25);
        		itemqty1.sendKeys(Integer.toString(randomNum1));
             } else if (i == 1) {
            	//giving random generator value up to 25 of double  
            	double randomNumber = new Random().nextDouble() * 25.0;
				WebElement itemqty = driver.findElement(By.id("item-request-quantity"));
				itemqty.sendKeys(String.format("%.2f", randomNumber));
            } else if (i == 2) {
            	//giving random generator value up to 50 of double  
            	double randomNumber = new Random().nextDouble() * 50.0;
				WebElement itemqty = driver.findElement(By.id("item-request-quantity"));
				itemqty.sendKeys(String.format("%.2f", randomNumber));
            }  else if (i == 3){
            	//giving random generator value up to 50 of integer 
            	WebElement itemqty1 = driver.findElement(By.id("item-request-quantity"));
        		int randomNum1 = random.nextInt(50);
        		itemqty1.sendKeys(Integer.toString(randomNum1));
            }else {
            	//giving random generator value up to 125 of double  
            	double randomNumber = new Random().nextDouble() * 125.0;
				WebElement itemqty = driver.findElement(By.id("item-request-quantity"));
				itemqty.sendKeys(String.format("%.3f", randomNumber));	
            }
			//clicking on save item button to save the items 
			driver.findElement(By.id("save-item-btn")).click();
			Thread.sleep(2000);
		}
		System.out.println(s);
		//going to items and selecting first item and deleting it 
		WebElement Elementitem = driver.findElement(By.id("item-id-1"));
		String itemnames1 = Elementitem.getText();
		s.remove(itemnames1);
		driver.findElement(By.id("delete-item1")).click();
		Thread.sleep(2000);
		driver.switchTo().alert().accept();
		Thread.sleep(2000);
		System.out.println(s);
		//going to items and selecting first item and it will check some edit and doing reset 
		driver.findElement(By.id("edit-item1")).click();
		Thread.sleep(2000);
		driver.findElement(By.id("reset-item-btn")).click();
		Thread.sleep(2000);
		//going to items and selecting first item and it will clear the quantity and giving the fresh  quantity whether it is double or integer and save the item 
		driver.findElement(By.id("edit-item1")).click();
		Thread.sleep(2000);
		driver.findElement(By.id("item-request-quantity")).clear();
		Thread.sleep(2000);
		double randomNumber1 = new Random().nextDouble() * 25.0;
		WebElement itemqty1 =driver.findElement(By.id("item-request-quantity"));
		itemqty1.sendKeys(String.format("%.2f",randomNumber1));
//		WebElement itemqty1 = driver.findElement(By.id("item-request-quantity"));
//		int randomNum1 = random.nextInt(20);
//		itemqty1.sendKeys(Integer.toString(randomNum1));
		driver.findElement(By.id("save-item-btn")).click();
		Thread.sleep(4000);
		System.out.println(s);
	}
  
  
		public void updateChecking(WebDriver driver) throws InterruptedException {

		for (int i = 0; i < 1; i++) {
			try {
//				calling indentupdating method from the same class 
				indentupdating(driver);
			} catch (Exception e){
				//after save as draft it is checking whether the items is adding or not if not it will be reset 
				driver.findElement(By.id("reset-item-btn")).click();
				Thread.sleep(2000);
			}
			Thread.sleep(2000);
			System.out.println("on loop no.:"+i);
		}
		System.out.println("out of loop");
	}
  
		public void indentupdating(WebDriver driver) throws InterruptedException {

		//again it is clicking on calendar date 
		driver.findElement(By.id("expected-delivery-date")).click();
		Thread.sleep(2000);
		//selecting some date in calendar field
		WebElement calendardate = driver.findElement(By.xpath("//*[@id='mat-datepicker-1']/div/mat-month-view/table/tbody/tr[5]/td[6]/button"));
		calendardate.click();
		Thread.sleep(2000);
		//clicking on same 
		driver.findElement(By.id("indentPurposeId")).click();
		Thread.sleep(2000);
		//selecting a random option from that options or fileds or elements 
		List<WebElement> indenttype = driver.findElements(By.xpath("//*[@id='indentPurposeId-panel']//mat-option"));
		Random random = new Random();
		int index = random.nextInt(indenttype.size());
		WebElement randomElement = indenttype.get(index);
		String indenttypeid = randomElement.getAttribute("id");
		randomElement.click();
		Thread.sleep(2000);
		//selecting  radiobutton options from that all radio buttons options 
		List<WebElement> indentcatgory = driver.findElements(By.xpath("//*[@id='indent-category']//mat-radio-button"));
		int index1 = random.nextInt(indentcatgory.size());
		WebElement randomElement1 = indentcatgory.get(index1);
		String indentcatgoryid = randomElement1.getAttribute("id");
		randomElement1.click();
		Thread.sleep(2000);
		//if the radiobutton is selected randomly  1st option means 
		if (indentcatgoryid.equals("indent-category-option-1")) {
			driver.findElement(By.id("company-type")).click();
			Thread.sleep(2000);
			List<WebElement> companytype = driver.findElements(By.xpath("//*[@id='company-type-panel']//mat-option"));
			int index3 = random.nextInt(companytype.size());
			WebElement randomElement3 = companytype.get(index3);
			String companytypeid = randomElement3.getAttribute("id");
			randomElement3.click();
			Thread.sleep(2000);
			driver.findElement(By.id("customer-id")).click();
			List<WebElement> selectcustomer = driver.findElements(By.xpath("//*[@id='customer-id-panel']//mat-option"));
			int index5 = random.nextInt(selectcustomer.size());
			WebElement randomElement5 = selectcustomer.get(index5);
			String selectcustomerid = randomElement5.getAttribute("id");
			randomElement5.click();
			Thread.sleep(2000);
			driver.findElement(By.id("project-id")).click();
			List<WebElement> projecttype = driver.findElements(By.xpath("//*[@id='project-id-panel']//mat-option"));
			Thread.sleep(2000);
			int index7 = random.nextInt(projecttype.size());
			WebElement randomElement7 = projecttype.get(index7);
			String projecttypeid = randomElement7.getAttribute("id");
			randomElement7.click();
			Thread.sleep(2000);
			driver.findElement(By.id("study-list")).click();
			List<WebElement> studylist = driver.findElements(By.xpath("//*[@id='study-list-panel']//mat-option"));
			int index9 = random.nextInt(studylist.size());
			WebElement randomElement9 = studylist.get(index9);
			String studylistid = randomElement9.getAttribute("id");
			randomElement9.click();
			Thread.sleep(2000);
		}
		//if the radiobutton is selected randomly  other  options means 
		//we have to create a set string for store the items and  not  to allow duplicate values 
		Set<String> s = new HashSet<String>();
		for (int i = 0; i < 2; i++) {
			// driver.findElement(By.id("item-name")).click();
			WebElement inputField = driver.findElement(By.id("item-name"));
			inputField.sendKeys("a");
			Thread.sleep(4000);
			List<WebElement> itemname = driver.findElements(By.xpath(" 	//*[@id='mat-autocomplete-0']//mat-option"));
			int indexitem = random.nextInt(itemname.size());
			System.out.println(indexitem);
			WebElement randomElementitem = itemname.get(indexitem);
			String itemnames = randomElementitem.getText();
			System.out.println(itemnames);
			randomElementitem.click();
			if (s.add(itemnames) == false) {
				System.out.println("duplicate value :" + indexitem);
				driver.findElement(By.id("reset-item-btn")).click();
				i--;
				continue;
			}
			s.add(itemnames);
			if (i == 0) {
				//giving random generator value up to 25 of integer 
            	WebElement itemqty1 = driver.findElement(By.id("item-request-quantity"));
        		int randomNum1 = random.nextInt(25);
        		itemqty1.sendKeys(Integer.toString(randomNum1));
             } else if (i == 1) {
            	//giving random generator value up to 25 of double  
            	double randomNumber = new Random().nextDouble() * 25.0;
				WebElement itemqty = driver.findElement(By.id("item-request-quantity"));
				itemqty.sendKeys(String.format("%.2f", randomNumber));
            }else {
            	//giving random generator value up to 125 of double  
            	double randomNumber = new Random().nextDouble() * 125.0;
				WebElement itemqty = driver.findElement(By.id("item-request-quantity"));
				itemqty.sendKeys(String.format("%.3f", randomNumber));
            }
			Thread.sleep(2000);
			//clicking on save item button to save the items
			driver.findElement(By.id("save-item-btn")).click();
			Thread.sleep(2000);
		}
		System.out.println(s);
		//going to items and selecting first item and deleting it 
		WebElement Elementitem = driver.findElement(By.id("item-id-1"));
		String itemnames1 = Elementitem.getText();
		s.remove(itemnames1);
		driver.findElement(By.id("delete-item1")).click();
		Thread.sleep(2000);
		driver.switchTo().alert().accept();
		Thread.sleep(2000);
		System.out.println(s);
		//going to items and selecting first item and it will check some edit and doing reset 
		driver.findElement(By.id("edit-item1")).click();
		Thread.sleep(2000);
		driver.findElement(By.id("reset-item-btn")).click();
		Thread.sleep(2000);
		//going to items and selecting first item and it will clear the quantity and giving the fresh  quantity whether it is double or integer and save the item
		driver.findElement(By.id("edit-item1")).click();
		Thread.sleep(2000);
		driver.findElement(By.id("item-request-quantity")).clear();
		Thread.sleep(2000);
//		double randomNumber1 = new Random().nextDouble() * 25.0;
//		WebElement itemqty1 =driver.findElement(By.id("item-request-quantity"));
//		itemqty1.sendKeys(String.format("%.2f",randomNumber1));
		WebElement itemqty1 = driver.findElement(By.id("item-request-quantity"));
		int randomNum1 = random.nextInt(20);
		itemqty1.sendKeys(Integer.toString(randomNum1));
		driver.findElement(By.id("save-item-btn")).click();
		Thread.sleep(3000);
		System.out.println(s);
			}
  
		public void approvalqty (WebDriver driver) throws InterruptedException {

			Random random=new Random();
			//selecting first indent from hambutger list 
			driver.findElement(By.id("pr-number-date-1")).click();
			Thread.sleep(2000);
			//creating for loop to give the approve qty of indent raised items 
			for (int i=1;i<=4;i++){
				if (i == 1) {
					//getting the req qty value and giving approve qty lessthan or equals to  with random value of  integer 
			    	WebElement reqqty = driver.findElement(By.id("item-req-qty-"+i));
					double reqqtyvalue = Double.valueOf(reqqty.getAttribute("value"));
					System.out.println("number=" +reqqtyvalue);
					WebElement appqty = driver.findElement(By.id("item-approved-qty-"+i));
					appqty.clear();
					double approvedqty = new Random().nextDouble() * reqqtyvalue;
					appqty.sendKeys(String.format("%.2f",approvedqty));
	            } else if (i == 2) {
		           	//getting the req qty value and giving approve qty lessthan or equals to  with random value of  integer            	
	            	WebElement reqqty = driver.findElement(By.id("item-req-qty-"+i));
	    			double reqqtyvalue = Double.valueOf(reqqty.getAttribute("value"));
	    			System.out.println("number=" +reqqtyvalue);
	    			WebElement appqty = driver.findElement(By.id("item-approved-qty-"+i));
	    			appqty.clear();
	    			double approvedqty = new Random().nextDouble() * reqqtyvalue;
					appqty.sendKeys(String.format("%.2f",approvedqty));
	            } else if (i == 3) {
	            	//getting the req qty value and giving approve qty lessthan or equals to  with random value of  integer	
	            	WebElement reqqty = driver.findElement(By.id("item-req-qty-"+i));
	    			double reqqtyvalue = Double.valueOf(reqqty.getAttribute("value"));
	    			System.out.println("number=" +reqqtyvalue);
	    			WebElement appqty = driver.findElement(By.id("item-approved-qty-"+i));
	    			appqty.clear();
	    			double approvedqty = new Random().nextDouble() * reqqtyvalue;
					appqty.sendKeys(String.format("%.2f",approvedqty));
	            }else{
	            	//getting the req qty value and giving approve qty lessthan or equals to  with random value of  integer or double 
	            	WebElement reqqty = driver.findElement(By.id("item-req-qty-"+i));
	    			double reqqtyvalue = Double.valueOf(reqqty.getAttribute("value"));
	    			System.out.println("number=" +reqqtyvalue);
	    			WebElement appqty = driver.findElement(By.id("item-approved-qty-"+i));
	    			appqty.clear();
	    			double approvedqty = new Random().nextDouble() * reqqtyvalue;
					appqty.sendKeys(String.format("%.2f",approvedqty));
	            }
			    Thread.sleep(3000);	
					}
			for (int i=1;i<=4;i++){
				//selcting all items by checkboxes options 
				driver.findElement(By.id("select-item-"+i)).click();	
				Thread.sleep(2000);
			}
			//before clicking  approve button we have to store the text of indent type in string variable  
			WebElement indentid1= driver.findElement(By.id("indentPurposeId"));
			String enteredText1 = indentid1.getText();		System.out.println("number=" +enteredText1);
			//clicking on approve biutton 
			driver.findElement(By.id("approve-btn")).click();	
			Thread.sleep(2000);
			
			// clicking on stores option 
			driver.findElement(By.id("stores")).click();
			Thread.sleep(2000);
			
			//if the indent type is in house it will go to if condition or it will go to else condition 
			if (enteredText1.equals("In-house Usage")) {
				driver.findElement(By.id("itemIssuance")).click();								//clicking on item issuance 
				Thread.sleep(2000);
				WebElement indentnumber = driver.findElement(By.id("indent-number"));			//selecting latest indent from drop  down options 
				Actions a=new Actions(driver);
				indentnumber.click();
				a.keyDown(Keys.ENTER).click().perform();
				Thread.sleep(2000);
				driver.findElement(By.id("reset-btn")).click();									//checking whether the reset button is working or not 
				Thread.sleep(2000);
				
				singleitemisuuance(driver);	Thread.sleep(2000);									//calling the method from the same  class 
				
				WebElement checkingindnum = driver.findElement(By.id("indent-number"));
				Actions a2=new Actions(driver);checkingindnum.click();
				a2.keyDown(Keys.ENTER).click().perform();Thread.sleep(2000);
				String indtnum=checkingindnum.getText();
				System.out.println(indtnum);
				Thread.sleep(2000);
				for (int i=1;i<=4;i++){
					driver.findElement(By.id("purchase")).click();
					Thread.sleep(2000);
					driver.findElement(By.id("purchaseRequisitionImg")).click();
					Thread.sleep(2000);
					WebElement indentnumid= driver.findElement(By.id("item-indent-number-1"));
					String indentnumberid = indentnumid.getAttribute("value");
					System.out.println("number=" +indentnumberid); Thread.sleep(2000);
					WebElement indenttypeid1= driver.findElement(By.id("item-indent-purpose-1"));
					String indent = indenttypeid1.getText();
					System.out.println("number=" +indent); 
					Thread.sleep(2000);
					if(enteredText1.equals(indent) && indtnum.equals(indentnumberid)) {				
						singleprandpo(driver,i);
					}
				}
//				driver.findElement(By.id("stores")).click();
//				Thread.sleep(2000);
//				driver.findElement(By.id("itemIssuance")).click();	
//				Thread.sleep(2000);
//				mutipleitemisuuance(driver);
//				Thread.sleep(2000);
				
				
				driver.findElement(By.id("purchase")).click();
				Thread.sleep(2000);
				driver.findElement(By.id("purchaseRequisitionImg")).click();
				Thread.sleep(2000);
				mutipleprandpo(driver);
				
					}else if (enteredText1.equals("External Sales")) {
				Thread.sleep(2000);
				driver.findElement(By.id("customerIssuanceImg")).click();
				Thread.sleep(2000);
				WebElement indentnumber1 = driver.findElement(By.id("indent-number"));
				Actions a1=new Actions(driver);
				indentnumber1.click();
				a1.keyDown(Keys.ENTER).click().perform();
				Thread.sleep(2000);
				driver.findElement(By.id("reset-btn")).click();
				Thread.sleep(2000);
				singlecustomerisuuance(driver);	Thread.sleep(2000);
				WebElement checkingindnum = driver.findElement(By.id("indent-number"));
				Actions a2=new Actions(driver);checkingindnum.click();
				a2.keyDown(Keys.ENTER).click().perform();Thread.sleep(2000);
				String indtnum=checkingindnum.getText();
				System.out.println(indtnum);
				Thread.sleep(2000);
				for (int i=1;i<=4;i++){
					driver.findElement(By.id("purchase")).click();
					Thread.sleep(2000);
					driver.findElement(By.id("purchaseRequisitionImg")).click();
					Thread.sleep(2000);
					WebElement indentnumid= driver.findElement(By.id("item-indent-number-1"));
					String indentnumberid = indentnumid.getAttribute("value");
					System.out.println("number=" +indentnumberid); Thread.sleep(2000);
					WebElement indenttypeid1= driver.findElement(By.id("item-indent-purpose-1"));
					String indent = indenttypeid1.getText();
					System.out.println("number=" +indent); 
					Thread.sleep(2000);
					if(enteredText1.equals(indent) && indtnum.equals(indentnumberid)) {				
						singleprandpo(driver,i);
					}
				}
					
//				driver.findElement(By.id("stores")).click();
//				Thread.sleep(2000);
//				driver.findElement(By.id("customerIssuanceImg")).click();
//				Thread.sleep(2000);
//				mutiplecustomerisuuance(driver);
//				Thread.sleep(2000);
				
				driver.findElement(By.id("purchase")).click();
				Thread.sleep(2000);
				driver.findElement(By.id("purchaseRequisitionImg")).click();
				Thread.sleep(2000);
				mutipleprandpo(driver);
				
					}	
			
		}

		public static void singleitemisuuance (WebDriver driver) throws InterruptedException {

			for (int i=1; i<=4;i++){
				WebElement indentnumber2 = driver.findElement(By.id("indent-number"));
				Actions a2=new Actions(driver);
				indentnumber2.click();
				a2.keyDown(Keys.ENTER).click().perform();
				Thread.sleep(2000);

				WebElement avaliblestockid = driver.findElement(By.id("item-stock-"+i));
				String avaliableqty=avaliblestockid.getAttribute("value");
				System.out.println(avaliableqty);
				Thread.sleep(2000);

				WebElement Allocatedid = driver.findElement(By.id("item-allocated-qty-pr-"+i));
				String Allocatedqty=Allocatedid.getAttribute("value");
				System.out.println(Allocatedqty);
				Thread.sleep(2000);

				WebElement ReqApprovedid = driver.findElement(By.id("item-req-approved-qty-"+i));
				String ReqApprovedqty=ReqApprovedid.getAttribute("value");
				System.out.println(ReqApprovedqty);
				Thread.sleep(2000);

				WebElement ReqPendingid	 = driver.findElement(By.id("item-req-pending-qty-"+i));
				String ReqPendingqty=ReqPendingid.getAttribute("value");
				System.out.println(ReqPendingqty);
				Thread.sleep(2000);

				double num1 =  Double.parseDouble(Allocatedqty);
				double num2 =  Double.parseDouble(ReqApprovedqty);
				double difference = num2 - num1; 
				String result = String.valueOf(difference/2);
				System.out.println(result);
				if (avaliableqty.equals("0")) {
					WebElement issueqty = driver.findElement(By.id("item-issue-qty-"+i));
					issueqty.clear();
					issueqty.sendKeys(result);
					Thread.sleep(2000);

					driver.findElement(By.id("select-item-"+i)).click();
					Thread.sleep(2000);
					driver.findElement(By.id("move-to-pr-btn")).click();
					Thread.sleep(2000);

				} else {
					driver.findElement(By.id("edit-item-"+i)).click();Thread.sleep(2000);	
					driver.findElement(By.id("batch-id")).click();Thread.sleep(2000);	
					Random random = new Random();
					List<WebElement> batchno = driver.findElements(By.xpath("//*[@id='batch-id-panel']//mat-option"));
					int batchsize = random.nextInt(batchno.size());
					WebElement batchtype = batchno.get(batchsize);
					batchtype.click();
					Thread.sleep(2000);

					//									driver.findElement(By.id("edit-item-"+i)).click();
					//									driver.findElement(By.id("batch-id")).click();
					//									List<WebElement> batchno1 = driver.findElements(By.xpath("//*[@id='batch-id-panel']//mat-option"));
					//									int batchsize1 = random.nextInt(batchno1.size());
					//									WebElement batchtype1 = batchno.get(batchsize1);
					//									batchtype1.click();
					//									Thread.sleep(2000);

					WebElement availablestockid = driver.findElement(By.id("available-stock"));
					double availablestockvalue = Double.valueOf(availablestockid.getAttribute("value"));
					System.out.println("number=" +availablestockvalue);Thread.sleep(2000);	

					WebElement requestedstockid = driver.findElement(By.id("requested-qty"));
					double requestedstockvalue = Double.valueOf(requestedstockid.getAttribute("value"));
					System.out.println("number=" +requestedstockvalue);Thread.sleep(2000);	

					if (availablestockvalue > requestedstockvalue) {
						WebElement issueqty = driver.findElement(By.id("issue-qty"));
						issueqty.clear();
						Thread.sleep(2000);	
						double issueedqty = new Random().nextDouble() * requestedstockvalue ;
						issueqty.sendKeys(String.format("%.3f",issueedqty));

					} else {
						WebElement issueqty = driver.findElement(By.id("issue-qty"));
						issueqty.clear();
						Thread.sleep(2000);	
						double issueedqty = new Random().nextDouble() * availablestockvalue ;
						issueqty.sendKeys(String.format("%.3f",issueedqty));

					} 
					Thread.sleep(2000);

					driver.findElement(By.id("add-item-btn")).click();	Thread.sleep(2000);
					driver.findElement(By.id("select-item-"+i)).click();	Thread.sleep(2000);
															driver.findElement(By.id("issue-btn")).click();		Thread.sleep(2000);															
				}

			}

		}

