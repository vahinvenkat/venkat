					//									WebElement batchtype1 = batchno.get(batchsize1);
					//									batchtype1.click();
					//									Thread.sleep(2000);

					WebElement availablestockid = driver.findElement(By.id("available-stock"));
					double availablestockvalue = Double.valueOf(availablestockid.getAttribute("value"));
					System.out.println("number=" +availablestockvalue);	Thread.sleep(2000);


					WebElement requestedstockid = driver.findElement(By.id("available-stock"));
					double requestedstockvalue = Double.valueOf(requestedstockid.getAttribute("value"));
					System.out.println("number=" +requestedstockvalue);	Thread.sleep(2000);

					if (availablestockvalue > requestedstockvalue) {
						WebElement issueqty = driver.findElement(By.id("issue-qty"));
						issueqty.clear();
						Thread.sleep(2000);	
						double issueedqty = new Random().nextDouble() * requestedstockvalue ;
						issueqty.sendKeys(String.format("%.3f",issueedqty));	Thread.sleep(2000);

					} else  {
						WebElement issueqty = driver.findElement(By.id("issue-qty"));
						issueqty.clear();
						Thread.sleep(2000);	
						double issueedqty = new Random().nextDouble() * availablestockvalue ;
						issueqty.sendKeys(String.format("%.3f",issueedqty));	Thread.sleep(2000);

					} 

					driver.findElement(By.id("add-item-btn")).click();	Thread.sleep(2000);
					driver.findElement(By.id("select-item-"+i)).click();
					Thread.sleep(2000);
					driver.findElement(By.id("issue-btn")).click();		Thread.sleep(2000);		
				}
			}
			for (int j=1; j<=3;j++){
			
				
					driver.findElement(By.id("select-item-"+j)).click();
					Thread.sleep(2000);

				}	 
				driver.findElement(By.id("move-to-pr-btn")).click();
				Thread.sleep(2000);
			}  		

