		public  static void singleprandpo (WebDriver driver, int i) throws InterruptedException{

			Random random = new Random();
			driver.findElement(By.id("item-supplier-1")).click();Thread.sleep(2000);
			driver.findElement(By.id("item-supplier-option-8-1")).click();Thread.sleep(2000);



			WebElement ReqPendingid1 = driver.findElement(By.id("item-pending-pr-qty-1"));
			String ReqPendingqty1=ReqPendingid1.getAttribute("value");
			System.out.println(ReqPendingqty1);
			Thread.sleep(2000);

			WebElement Reqqty = driver.findElement(By.id("item-req-qty-1"));Reqqty.clear();
			String Reqqtynum=Reqqty.getAttribute("value");Reqqty.sendKeys(ReqPendingqty1);
			System.out.println(ReqPendingqty1);Thread.sleep(2000);

			driver.findElement(By.id("select-item-1")).click();Thread.sleep(2000);
			driver.findElement(By.xpath("/html/body/app-root/app-main-module/app-sidebar/mat-sidenav-container/mat-sidenav-content/main/div/app-purchase-requisition/mat-drawer-container/mat-drawer-content/div/div[2]/div/button[1]/span[1]")).click();Thread.sleep(2000);

			driver.findElement(By.id("pr-number-date1")).click();Thread.sleep(2000);
			//						driver.findElement(By.id("reset-button")).click();Thread.sleep(2000);
			//						driver.findElement(By.id("pr-number-date1")).click();Thread.sleep(2000);

			WebElement requisitionid = driver.findElement(By.id("requisition-serial-code"));
			String requisitionnum=requisitionid.getAttribute("value");
			System.out.println(ReqPendingqty1);	Thread.sleep(2000);

			try
			{
				driver.findElement(By.id("/html/body/app-root/app-main-module/app-sidebar/mat-sidenav-container/mat-sidenav-content/main/div/app-purchase-requisition/mat-drawer-container/mat-drawer-content/div/div[2]/div/button[2]/span[1]")).click();Thread.sleep(2000); 
			}
			catch(Exception e)
			{
				driver.findElement(By.id("move-to-po")).click();
			}

			driver.findElement(By.id("purchase")).click();
			Thread.sleep(2000);
			driver.findElement(By.id("purchaseOrderImg")).click();
			Thread.sleep(2000);
			WebElement prnum = driver.findElement(By.id("pr-master-id"));
			Actions a1=new Actions(driver);
			prnum.click();
			a1.keyDown(Keys.ENTER).click().perform();
			Thread.sleep(2000);

			driver.findElement(By.id("supplier-id")).click();
			Thread.sleep(2000);
			List<WebElement> suppliertype1 = driver.findElements(By.xpath("//*[@id='supplier-id-panel']//mat-option"));
			int supplier1 = random.nextInt(suppliertype1.size());
			WebElement supplierrandom1 = suppliertype1.get(supplier1);
			String suppliertypeid1 = supplierrandom1.getText();
			supplierrandom1.click();  Thread.sleep(2000);

			driver.findElement(By.id("po-date")).click();Thread.sleep(2000);
			driver.findElement(By.xpath("//*[@aria-label='June 30, 2023']")).click();   //select date
			Thread.sleep(2000); 
			driver.findElement(By.id("payment-term-id")).click();                                                                        //click on payment terms
			Thread.sleep(2000); 
			List<WebElement> PayTerm = driver.findElements(By.xpath("//*[@id='payment-term-id-panel']//mat-option"));                    //select random payment term
			Random rand = new Random();
			int index = rand.nextInt(PayTerm.size());
			WebElement ranElem = PayTerm.get(index);
			String id = ranElem.getAttribute("id");
			ranElem.click();
